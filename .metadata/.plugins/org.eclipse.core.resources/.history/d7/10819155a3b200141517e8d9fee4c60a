package com.iiitb.factory;

import org.w3c.dom.NodeList;

import com.iiitb.blocks.Block;

public class SumHelper implements Helper{


	@Override
			public void setAttr(NodeList attributes, int iter, Block block) {
				// TODO Auto-generated method stub
				if(attributes.item(iter)
						.getTextContent().replaceAll("|","").contains("++"))
						{
							block.setSign(1);
						}
						
						else if(attributes.item(iter)
								.getTextContent().replaceAll("|","").contains("+-"))
						{
							block.setSign(2);
						}
						
						else
						{
							
							block.setSign(3);
						}
			}
}
