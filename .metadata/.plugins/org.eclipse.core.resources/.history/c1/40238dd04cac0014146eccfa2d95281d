package com.iiitb.test;

import org.w3c.dom.Document;

import com.iiitb.cfg.Accfg;
import com.iiitb.utility.ParseXML;

public class TestFeature {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			Document doc = ParseXML
					.initializeDocument("C:/Users/vsriganesh/Documents/MATLAB/model_code.xml");
			
			/* String root is mentioned so as to mention the top level subsystem
			 String subsystem will be used for inner subsystem and count refers to the 
														subsystem level*/
			
			Accfg mergedAccfg = ParseXML.parseDocument(doc, "root", ParseXML.countSubSystem);
			
			System.out.println(mergedAccfg);
		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
