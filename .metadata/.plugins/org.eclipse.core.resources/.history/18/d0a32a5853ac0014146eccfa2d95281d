package com.iiitb.test;

import org.w3c.dom.Document;

import com.iiitb.cfg.Accfg;
import com.iiitb.utility.ParseXML;

public class TestFeature {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			Document doc = ParseXML
					.initializeDocument("C:/Users/vsriganesh/Documents/MATLAB/model_code.xml");

			/*
			 * String root is mentioned so as to mention the top level subsystem
			 * String subsystem will be used for inner subsystem and count
			 * refers to the subsystem level
			 * 
			 * The currSubSystemNode represents the current subsystem node. For the first call since 
			 * we are at the top most level we pass null to currSubSystemNode
			 */

			/*
			 * For the first call ParseXML.countSubSystem=0 (i.e) we are dealing
			 * with the top level subsystem
			 */
			Accfg mergedAccfg = ParseXML.parseDocument(doc, "root",
					ParseXML.countSubSystem,null);

			System.out.println(mergedAccfg);
		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
